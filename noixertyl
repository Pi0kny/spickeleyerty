local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Bloxfruits Script Hub", "GrapeTheme")

local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player Hacks")

PlayerSection:NewButton("Reset", "Reset your character", function()
    game.Players.LocalPlayer.Character.Head:Destroy()
end)

PlayerSection:NewSlider("Player Speed", "Changes Player Walkspeed.", 500, 0, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("Player Jump", "Changes Player Jump Height.", 500, 0, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewButton("Infinite Jump Script", "Allows you to Infinitely Jump", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/V3DKCQTT"))()
end)

PlayerSection:NewButton("Serverhop", "Serverhops to the smallest server.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/ze1NtMkp"))()
end)
local Autofarm = Window:NewTab("Autofarm")
local Autofarm = Autofarm:NewSection("Autofarm")

Autofarm:NewButton("KayMobile Autofarm", "Launches the Player Hunter Script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/AXCScript/KayMobile/main/Script-Loader'))()
end)

Autofarm:NewButton("Player Hunter Script", "Launches the Player Hunter Script.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Legenda06/blox-fruits/main/chida"))()
end)

local FruitFinder = Window:NewTab("Fruit Finder")
local FruitFinderSection = FruitFinder:NewSection("Fruit Farms:")

FruitFinderSection:NewButton("Fruit Finder", "Launches the Fruit Finder Script", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/wGkLw2Ta"))()
end)

local CreditTab = Window:NewTab("Credits")
local CreditSection = CreditTab:NewSection("Created By:")

CreditSection:NewLabel("Scripter: Piokny")
CreditSection:NewLabel("Tester: Fataldarksage")
CreditSection:NewLabel("Our Discord: (Code here)")
CreditSection:NewLabel("Last Updated: 9/10/2023")
