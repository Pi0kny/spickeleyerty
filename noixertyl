local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Bloxfruits Script Hub", "GrapeTheme")

local Main = Window:NewTab("Main")

local Autofarm = Window:NewTab("Autofarm")
local Autofarm = Autofarm:NewSection("Autofarm")

Autofarm:NewButton("KayMobile Autofarm", "Launches the Player Hunter Script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/AXCScript/KayMobile/main/Script-Loader'))()
end)

Autofarm:NewButton("Player Hunter Script", "Launches the Player Hunter Script.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Legenda06/blox-fruits/main/chida"))()
end)

local FruitFinder = Window:NewTab("Fruit Finder")
local FruitFinderSection = FruitFinder:NewSection("Fruit Farms:")

FruitFinderSection:NewButton("Fruit Finder #1", "Launches the Fruit Finder #1 Script", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/wGkLw2Ta"))()
end)

FruitFinderSection:NewButton("Fruit Finder #2", "Launches the Fruit Finder #2 Script", function()
    loadstring(game:HttpGet"https://gist.githubusercontent.com/NotHubris/4e6fdc88d84c30afa9b28c590f273bbf/raw")()
end)

local Miscellaneous = Window:NewTab("Miscellaneous")
local MiscellaneousSection = Miscellaneous:NewSection("Miscellaneous Hacks:")

MiscellaneousSection:NewButton("Reset", "Reset your character", function()
    game.Players.LocalPlayer.Character.Head:Destroy()
end)

MiscellaneousSection:NewSlider("Player Speed", "Changes Player Walkspeed.", 500, 0, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MiscellaneousSection:NewSlider("Player Jump", "Changes Player Jump Height.", 500, 0, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

MiscellaneousSection:NewButton("Infinite Jump Script", "Allows you to Infinitely Jump", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/V3DKCQTT"))()
end)

MiscellaneousSection:NewButton("Serverhop", "Serverhops to the smallest server.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/ze1NtMkp"))()
end)

MiscellaneousSection:NewButton("Infinite Yield Admin", "Gives Inf Yield admin commands.", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

MiscellaneousSection:NewButton("Cmd-X Admin", "Gives Cmd-X admin  commands.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)

MiscellaneousSection:NewButton("Fates Admin", "Gives Fates admin  commands.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
end)

local CreditTab = Window:NewTab("Credits")
local CreditSection = CreditTab:NewSection("Created By:")

CreditSection:NewLabel("Scripter: Piokny")
CreditSection:NewLabel("Script Tester: Fataldarksage")
CreditSection:NewLabel("Our Discord: (Code here)")
CreditSection:NewLabel("Last Updated: 9/10/2023")
